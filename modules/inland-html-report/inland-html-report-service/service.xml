<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="org.opencps.inland">
	<namespace>inland</namespace>
	<entity table="inland_print_template" local-service="true" name="InlandPrintTemplate" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="printTemplateId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="createUserId" type="long" />
		<column name="employeeId" type="long" />

		<column name="serviceCode" type="String" />
		<column name="dossierPartNo" type="String" />
		<column name="fileTemplateNo" type="String" />
		<column name="templateNo" type="String" />
		<column name="formTemplate" type="String" />
		<column name="defaultCss" type="String" />		
		<column name="originalDocumentURL" type="String" />
		<column name="licenceType" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="createDate" />
		</order>
        <finder name="SC_TN_PN_FTN" return-type="InlandPrintTemplate">
            <finder-column name="createUserId"></finder-column>
            <finder-column name="serviceCode"></finder-column>
            <finder-column name="templateNo"></finder-column>
            <finder-column name="dossierPartNo"></finder-column>
            <finder-column name="fileTemplateNo"></finder-column>
        </finder>

        <finder name="SC_TN_PN_FTN_LT" return-type="InlandPrintTemplate">
            <finder-column name="createUserId"></finder-column>
            <finder-column name="serviceCode"></finder-column>
            <finder-column name="templateNo"></finder-column>
            <finder-column name="dossierPartNo"></finder-column>
            <finder-column name="fileTemplateNo"></finder-column>
            <finder-column name="licenceType"></finder-column>
        </finder>

	</entity>
</service-builder>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ten_loi">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="5000"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="Content" type="Content" />
	<xsd:complexType name="Content">
		<xsd:sequence>
			<xsd:element ref="DS_Loi" minOccurs="0" />
			<xsd:element ref="KetQuaXuLy" minOccurs="0" />			
			<xsd:element ref="Phanhoi_Yeucau_Sua" minOccurs="0" />
			<xsd:element ref="VLInterRoadTransportLicence" minOccurs="0"></xsd:element>
			<xsd:element ref="VLCrossBorderTransportPermit" minOccurs="0"></xsd:element>
			<xsd:element ref="ApprovalOfVLFixRoadTransport" minOccurs="0"></xsd:element>
			<xsd:element ref="ApprovalOfVLStopTransport" minOccurs="0"></xsd:element>
			<xsd:element ref="ApprovalOfVLAdjustFrequency" minOccurs="0"></xsd:element>
			<xsd:element ref="VTOfficialTransportPermit" minOccurs="0"></xsd:element>
			<xsd:element ref="ApprovalOfVTFixRoadTransport" minOccurs="0"></xsd:element>
			<xsd:element ref="CLVCommercialCrossBorderTransportPermit" minOccurs="0"></xsd:element>
			<xsd:element ref="CLVNonCommercialCrossBorderTransportPermit" minOccurs="0"></xsd:element>
			<xsd:element ref="ApprovalOfCLVFixRoadTransport" minOccurs="0"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="KetQuaXuLy" type="KetQuaXuLy"/>
